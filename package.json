{
  "name": "condohub",
  "private": true,
  "engines": {
    "node": "^14.19 || ^16.13"
  },
  "workspaces": {
    "packages": [
      "packages/common",
      "packages/apps-script",
      "packages/server",
      "packages/website"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "prebuild": "yarn lerna:clean",
    "build": "yarn lerna:build",
    "changelog": "nr lerna-version '--conventional-commits --no-push'",
    "clean": "yarn lerna:clean",
    "commit": "lint-staged && git-cz",
    "deduplicate": "npx yarn-deduplicate yarn.lock && yarn",
    "depcheck": "yarn lerna:depcheck",
    "format:changelog": "prettier --write packages/**/CHANGELOG.md --loglevel silent && git add packages/**/CHANGELOG.md",
    "lerna:build": "lerna exec yarn build --scope @condohub/common @condohub/server",
    "lerna:clean": "lerna exec yarn clean",
    "lerna:depcheck": "lerna exec --concurrency=1 nr depcheck",
    "lerna:ls-lint": "lerna exec --concurrency=1 ls-lint",
    "lerna:start": "lerna exec yarn start",
    "lerna:test": "lerna exec yarn test",
    "lerna:test:e2e": "lerna exec yarn --concurrency=1 test:e2e",
    "lerna:typecheck": "lerna exec yarn typecheck --concurrency=4 --loglevel info",
    "link:core-cli": "npm --prefix packages/core-cli link",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "ls-lint": "ls-lint",
    "postinstall": "patch-package && yarn build",
    "push": "git push --follow-tags",
    "release": "env-cmd -x nr lerna-version '--conventional-commits --create-release github'",
    "release:test": "env-cmd -x echo $GH_TOKEN",
    "reset": "rimraf -rf ./node_modules packages/**/node_modules && yarn",
    "prestart": "yarn lerna:clean",
    "start": "yarn lerna:start",
    "test": "yarn lerna:test",
    "typecheck": "yarn lerna:typecheck"
  },
  "resolutions": {
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@jest/transform": "^27",
    "@types/jest": "^27",
    "@types/react": "^18",
