{
  "name": "condohub",
  "private": true,
  "engines": {
    "node": "^16.13"
  },
  "workspaces": {
    "packages": [
      "packages/common-utils",
      "packages/node-utils",
      "packages/jest-config",
      "packages/apis",
      "packages/models",
      "packages/apps-script",
      "packages/cli",
      "packages/server",
      "packages/website"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build": "yarn lerna:build:lib",
    "changelog": "condohub  lerna-version '--conventional-commits --no-push'",
    "clean": "yarn lerna:clean:lib",
    "cli": "packages/cli/bin/run.js",
    "commit": "lint-staged && git-cz",
    "depcheck": "yarn lerna:depcheck",
    "format:changelog": "prettier --write packages/**/CHANGELOG.md --loglevel silent && git add packages/**/CHANGELOG.md",
    "lerna:build:app": "lerna exec yarn build --scope @condohub/app",
    "lerna:build:cli": "lerna exec --stream --scope @condohub/cli -- yarn build",
    "lerna:build:lib": "lerna exec --stream --scope @condohub/common-utils --scope @condohub/jest-config --scope @condohub/node-utils --scope @condohub/common --scope @condohub/apis --scope @condohub/models -- yarn build",
    "lerna:build:server": "lerna exec --stream --scope @condohub/server -- yarn build",
    "lerna:clean": "lerna exec --stream -- yarn clean",
    "lerna:clean:lib": "lerna exec --stream --scope @condohub/common-utils --scope @condohub/jest-config --scope @condohub/node-utils --scope @condohub/common -- yarn clean",
    "lerna:depcheck": "lerna exec --concurrency=1 condohub depcheck",
    "lerna:ls-lint": "lerna exec --concurrency=1 ls-lint",
    "lerna:start:app": "lerna exec --scope @condohub/app -- yarn start",
    "lerna:start:lib": "lerna exec --parallel --scope @condohub/common-utils --scope @condohub/jest-config --scope @condohub/node-utils --scope @condohub/common --scope @condohub/apis --scope @condohub/models -- yarn start",
    "lerna:start:server": "lerna exec --stream --scope @condohub/server -- yarn start",
    "lerna:test": "lerna run test",
    "lerna:test:e2e": "lerna exec --concurrency=1 yarn test:e2e",
    "lerna:typecheck": "lerna exec --concurrency=1 --loglevel info yarn tsc:typecheck",
    "link:cli": "npm --prefix packages/cli link",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "ls-lint": "ls-lint",
    "patch-package": "patch-package --exclude 'nothing'",
    "postinstall": "patch-package && yarn build && yarn link:cli",
    "push": "git push --follow-tags",
    "release": "env-cmd -x condohub  lerna-version '--conventional-commits --create-release github'",
    "release:test": "env-cmd -x echo $GH_TOKEN",
    "reset": "npm i rimraf && rimraf -rf ./node_modules packages/**/node_modules && npm i",
    "prestart": "yarn build",
    "start": "yarn lerna:start:lib",
    "test": "yarn lerna:test",
    "typecheck": "yarn lerna:typecheck"
  },
  "overrides": {
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@jest/transform": "^27",
    "@types/jest": "^27",
    "@types/react": "^18",
