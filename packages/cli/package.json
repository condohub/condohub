{
  "name": "@condohub/cli",
  "version": "1.0.1",
  "description": "CLI utility for advanced usage",
  "author": {
    "email": "info@condohub.io",
    "name": "CondoHub",
    "url": "https://github.com/condohub/condohub"
  },
  "homepage": "https://github.com/condohub/condohub",
  "repository": {
    "directory": "packages/core-cli",
    "type": "git",
    "url": "https://github.com/condohub/condohub.git"
  },
  "license": "MIT",
  "bugs": "https://github.com/https://github.com/condohub/condohub/issues",
  "bin": {
    "condocli": "./bin/run.js"
  },
  "main": "src/index.js",
  "module": "src/index.js",
  "files": [
    "/bin",
    "/oclif.manifest.json",
    "/src",
    "/types"
  ],
  "scripts": {
    "build": "yarn prepack",
    "clean": "rimraf -rf lib/** && yarn postpack && rimraf -f *.tsbuildinfo",
    "cli": "env-cmd -x ./bin/run.js",
    "manifest": "env-cmd -x ./bin/run.js",
    "postpack": "rimraf -f oclif.manifest.json",
    "prepack": "rimraf -rf lib && yarn tsc:build && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "tsc:build": "tsc -b",
    "tsc:build:watch": "yarn tsc:build -w",
    "tsc:typecheck": "tsc --noEmit --composite false --incremental false"
  },
  "exports": {
    ".": "./src/index.js",
    "./package.json": "./package.json",
    "./types": "./types/index.d.ts"
  },
  "oclif": {
    "bin": "condocli",
    "commands": "./lib/commands",
    "plugins": []
  },
  "types": "./types/index.d.ts",
  "dependencies": {
    "@condohub/node-utils": "*",
    "@oclif/core": "^1",
    "io-ts": "^2",
    "typescript": "^4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0"
  }
}
