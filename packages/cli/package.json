{
  "name": "@condohub/cli",
  "version": "1.0.1",
  "description": "CLI utility for advanced usage",
  "author": {
    "email": "info@condohub.io",
    "name": "CondoHub",
    "url": "https://github.com/condohub/condohub"
  },
  "homepage": "https://github.com/condohub/condohub",
  "repository": {
    "directory": "packages/core-cli",
    "type": "git",
    "url": "https://github.com/condohub/condohub.git"
  },
  "license": "MIT",
  "bugs": "https://github.com/https://github.com/condohub/condohub/issues",
  "bin": {
    "condocli": "./bin/run"
  },
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/oclif.manifest.json",
    "/src",
    "/types"
  ],
  "scripts": {
    "build": "yarn prepack",
    "clean": "rimraf -rf dist/** && yarn postpack && rimraf -f *.tsbuildinfo",
    "cli": "env-cmd -x ./bin/run",
    "dev": "env-cmd -x ./bin/dev",
    "postpack": "rimraf -f oclif.manifest.json",
    "prepack": "rimraf -rf lib && yarn tsc:build && oclif manifest && oclif readme",
    "start": "yarn tsc:build:watch",
    "test": "jest",
    "tsc:build": "tsc -b",
    "tsc:build:watch": "yarn tsc:build -w",
    "tsc:typecheck": "tsc --noEmit --composite false --incremental false",
    "version": "oclif readme && git add README.md"
  },
  "exports": {
    ".": "./src/index.js",
    "./package.json": "./package.json",
    "./types": "./types/index.d.ts"
  },
  "oclif": {
    "bin": "condocli",
    "commands": "./dist/commands",
    "dirname": "condocli",
    "flexibleTaxonomy": true,
    "hooks": {
      "command_incomplete": "./dist/hooks/command-incomplete",
      "command_not_found": "./dist/hooks/command-not-found",
      "init": "./dist/hooks/init",
      "postrun": "./dist/hooks/postrun",
      "prerun": "./dist/hooks/prerun"
    },
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "auth": {
        "description": "Manage authentication to your CondoHub account and your Google Workspace"
      },
      "building": {
        "description": "Add edit or remove buildings."
      }
    }
  },
  "types": "./types/index.d.ts",
  "dependencies": {
    "@condohub/apis": "*",
    "@condohub/models": "*",
    "@condohub/node-utils": "*",
    "@oclif/config": "^1",
    "@oclif/core": "^1",
    "inquirer": "^8",
    "io-ts": "^2",
    "listr2": "^4",
    "oclif": "^3",
    "typescript": "^4"
  }
}
