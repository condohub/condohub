{
  "name": "@condohub/server",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "npm run tsc:build && npm run swc:build",
    "clean": "npm run clean:dist && npm run clean:tsc",
    "clean:dist": "rimraf -rf dist/**",
    "clean:tsc": "rimraf -f ./**/*.tsbuildinfo *.tsbuildinfo",
    "nodemon": "nodemon  --delay 2000ms --watch dist/ --ext js,ts,tsx --ignore 'dist/*.d.ts' --exec 'npm run server'",
    "server": "env-cmd -x cross-env TS_NODE_PROJECT=../../tsconfig.node-cli.json node dist/server.js",
    "server:kill-port": ". ./.env && kill-port --port $APP_PORT && kill-port --port 9229",
    "start": "concurrently \"npm run tsc:build:watch\" \"npm run swc:build:watch\" \"npm run nodemon\"",
    "swc:build": "swc ./src --config-file ../../.swcrc --source-maps --ignore **/__test__/**/* --out-dir dist",
    "swc:build:watch": "npm run swc:build --watch",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --watch",
    "tsc:build": "tsc --emitDeclarationOnly",
    "tsc:build:watch": "npm run tsc:build --watch --preserveWatchOutput",
    "tsc:typecheck": "tsc --noEmit",
    "typecheck": "npm run tsc:typecheck"
  },
  "sideEffects": false,
  "type": "module",
  "dependencies": {
    "@condohub/apis": "*",
    "@condohub/models": "*",
    "google-auth-library": "^8",
    "googleapis": "^100.0.0"
  }
}
